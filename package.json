{
  "name": "happykit",
  "version": "1.1.1",
  "description": "happykit for admin",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "tsc -w --outDir ./example/src/lib",
    "build": "tsc -b tsconfig.build.json",
    "test": "jest --coverage",
    "format": "prettier --write src/**/*.ts",
    "lint": "tslint -p tsconfig.json",
    "copy": "cp -R lib/ example/src/lib/",
    "release": "standard-version",
    "publish-npm": "git push --follow-tags origin master && yarn publish"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "vue3js",
    "admin",
    "framework"
  ],
  "contributors": [
    {
      "name": "pumelotea",
      "email": "zf_722@yeah.net"
    }
  ],
  "bugs": {
    "url": "https://github.com/pumelotea/happykit/issues",
    "email": "zf_722@yeah.net"
  },
  "author": "pumelotea",
  "license": "Apache",
  "repository": {
    "type": "git",
    "url": "https://github.com/pumelotea/happykit.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint -p tsconfig.json",
      "prettier --write src/**/*.ts"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^3.6.5",
    "js-md5": "0.7.3"
  },
  "devDependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.17",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "standard-version": "^4.4.0",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.3"
  }
}
