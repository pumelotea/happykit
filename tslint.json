{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "ordered-imports": false,
    "no-namespace": false
  },
  "linterOptions": {
    "exclude": [
      "**/*.json",
      "node_modules",
      "example"
    ]
  }
}
